{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "hms": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/hms",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets",
              "src/fonts"
            ],
            "styles": [ 
              "src/styles.css",
              "src/assets/css/animate.min.css",
              "src/assets/css/all.min.css",
              "src/assets/css/aos.web.css",
              "src/assets/css/asap.css",
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/bootstrap.min.web.css",
              "src/assets/css/checkout.css",
              "src/assets/css/dark.web.css",
              "src/assets/css/datepicker.min.css",
              "src/assets/css/flaticon.web.css",
              "src/assets/css/fullcalendar.min.css",
              "src/assets/css/increment.css",
              "src/assets/css/index-style.web.css",
              "src/assets/css/jquery.dataTables.min.css",
              "src/assets/css/magnific-popup.web.css",
           "src/assets/fonts/flaticon.css",
              "src/assets/css/main.css",
              "src/assets/css/meanmenu.web.css",
              "src/assets/css/normalize.css",
              "src/assets/css/odometer.min.web.css",
              "src/assets/css/owl.carousel.min.web.css",
              "src/assets/css/owl.theme.default.min.web.css",
              "src/assets/css/popup.css",
              "src/assets/css/remixicon.web.css",
              "src/assets/css/reservations.css",
              "src/assets/css/responsive.web.css",
              "src/assets/css/select2.min.css",
              "src/assets/css/style.css"
           ],
           "scripts": [
            "src/assets/js/aos.web.js",
            "src/assets/js/ajaxchimp.min.web.js",
            "src/assets/js/appear.min.web.js",
            "src/assets/js/bootstrap.bundle.min.web.js",
            "src/assets/js/bootstrap.min.js",
            "src/assets/js/carousel-thumbs.min.web.js",
            "src/assets/js/Chart.min.js",
            "src/assets/js/contact-form-script.web.js",
            "src/assets/js/custom.web.js",
            "src/assets/js/datepicker.min.js",
            "src/assets/js/form-validator.min.web.js",
            "src/assets/js/fullcalendar.min.js",
            "src/assets/js/google-marker-map.js",
            "src/assets/js/jquery-3.3.1.min.js",
            "src/assets/js/jquery.counterup.min.js",
            "src/assets/js/jquery.dataTables.min.js",
            "src/assets/js/jquery.magnific-popup.web.js",
            "src/assets/js/jquery.meanmenu.web.js",
            "src/assets/js/jquery.min.web.js",
            "src/assets/js/jquery.scrollUp.min.js",
            "src/assets/js/jquery.waypoints.min.js",
            "src/assets/js/main.js",
            "src/assets/js/modernizr-3.6.0.min.js",
            "src/assets/js/moment.min.js",
            "src/assets/js/odometer.min.web.js",
            "src/assets/js/owl.carousel.min.web.js",
            "src/assets/js/plugins.js",
            "src/assets/js/popper.min.js",
            "src/assets/js/select2.min.js"
         ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "hms:build:production"
            },
            "development": {
              "browserTarget": "hms:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "hms:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "91a28f51-b600-4d54-ae3c-04518e1e203a"
  }
}
